{
  "name": "minhoteca",
  "version": "1.0.0",
  "description": "API para empréstimo de livros particulares",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test-ant": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --detectOpenHandles --coverage --reporters=default --passWithNoTests",
    "test:staged": "NODE_OPTIONS=--experimental-vm-modules jest --findRelatedTests --bail",
    "test:staged-ant": "NODE_OPTIONS=--experimental-vm-modules jest --findRelatedTests --bail",
    "test:coverage": "jest --coverage",
    "test:coverage-ant": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:ci": "jest --ci --detectOpenHandles --coverage --reporters=jest-junit --reporters=default --passWithNoTests || true",
    "prepare": "husky",
    "sonar": "npm run test:coverage && sonarqube-scanner"
  },
  "keywords": [
    "Minhoteca",
    "biblioteca",
    "livro",
    "empréstimo"
  ],
  "author": "Gustavo Adolfo A. S.",
  "license": "ISC",
  "devDependencies": {
    "@aws-crypto/client-node": "^4.2.1",
    "@aws-sdk/client-cognito-identity-provider": "^3.922.0",
    "@aws-sdk/client-config-service": "^3.922.0",
    "@aws-sdk/client-dynamodb": "^3.922.0",
    "@aws-sdk/client-s3": "^3.922.0",
    "@aws-sdk/client-ses": "^3.922.0",
    "@aws-sdk/client-sns": "^3.922.0",
    "@aws-sdk/lib-dynamodb": "^3.927.0",
    "@aws-sdk/node-http-handler": "^3.370.0",
    "@aws-sdk/s3-request-presigner": "^3.922.0",
    "@aws-sdk/types": "^3.922.0",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.39.0",
    "@faker-js/faker": "^10.1.0",
    "@jest/globals": "^30.2.0",
    "@types/aws-lambda": "^8.10.157",
    "@types/config": "^3.3.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.2",
    "aws-jwt-verify": "^5.1.1",
    "aws-lambda": "^1.0.7",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "sonarqube-scanner": "^4.3.2",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript-eslint": "^8.46.2"
  },
  "dependencies": {
    "winston": "^3.18.3"
  }
}
